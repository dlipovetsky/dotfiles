[include]
	path = "~/.gitconfig.personal"
	path = "~/.gitconfig.d2iq"
[init]
	defaultBranch = main
[rebase]
	autosquash = true
	autostash = true
[color "diff"]
	whitespace = red reverse
[diff]
	wsErrorHighlight = all
	colorMoved = zebra
	algorithm = histogram
[push]
	default = current
[url "https://github.com/"]
	insteadOf = git@github.com:
[core]
	editor = vim
[pull]
	ff = only
[credential "https://github.com"]
	helper =
	helper = !/usr/bin/env gh auth git-credential
